{
  "name": "elitehub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn run start-ssl & next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "start-ssl": "local-ssl-proxy --source 3010 --target 3000 --cert localhost.pem --key localhost-key.pem",
    "cms:login": "storyblok login",
    "cms:logout": "storyblok logout",
    "generate:theme-types": "cross-env NODE_PATH=src/ chakra-cli tokens src/theme/theme.ts",
    "generate:storyblok-component-types": "storyblok pull-components --space=231095 && storyblok-generate-ts source=./components.231095.json target=./src/generated/storyblok-components.ts",
    "generate:all-types": "npm-run-all -p generate:theme-types generate:storyblok-component-types"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.4.3",
    "@chakra-ui/theme-tools": "^2.0.12",
    "@emotion-icons/boxicons-logos": "^3.19.0",
    "@emotion-icons/boxicons-regular": "^3.19.0",
    "@emotion-icons/boxicons-solid": "^3.19.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/rubik": "^4.5.11",
    "@fontsource/saira": "^4.5.11",
    "@n8tb1t/use-scroll-position": "^2.0.3",
    "@storyblok/react": "^2.1.0",
    "framer-motion": "^7.6.19",
    "next": "13.4.3",
    "polished": "^4.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.4.1",
    "@sudolabs-io/eslint-config-sudolabs": "1.5.116",
    "@types/node": "18.11.0",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "13.4.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "26.9.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-lodash": "^7.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "storyblok": "^3.24.0",
    "storyblok-generate-ts": "^1.14.0",
    "typescript": "^4.9.4"
  }
}
